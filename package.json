{
  "name": "enumconsts",
  "version": "1.1.0",
  "description": "Just node.js consts as an enum. Nothing more.",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts"
  ],
  "scripts": {
    "check-code": "npm run format:check && npm run lint:check",
    "format": "prettier --write '**/*.js'",
    "format:check": "prettier --check '**/*.js'",
    "generate-types": "tsc",
    "lint": "eslint --fix '**/*.js'",
    "lint:check": "eslint '**/*.js'",
    "prepublishOnly": "npm run check-code && npm run generate-types",
    "preversion": "npm run check-code && npm run generate-types",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panta82/enumconsts.git"
  },
  "keywords": [
    "enum",
    "consts",
    "constants",
    "enoent",
    "errors",
    "signals",
    "codes",
    "error-codes",
    "error-constants"
  ],
  "author": "Ivan Pantic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/panta82/enumconsts/issues"
  },
  "homepage": "https://github.com/panta82/enumconsts#readme",
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^1.18.2",
    "typescript": "^4.4.3"
  }
}
